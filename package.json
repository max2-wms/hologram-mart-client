{
  "name": "hologram-mart-client",
  "version": "0.0.0",
  "description": "Hologram-Mart Client: Celebrity Hologram Store application which includes a frontend and backend API, along with a database.",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "docker-compose -f compose/docker-compose.dev.yml up",
    "stop": "docker-compose -f compose/docker-compose.dev.yml down",
    "build:images": "docker-compose -f compose/docker-compose.build.yml build",
    "build:image": "docker build -t max2wms/hologram-mart-client .",
    "push:image": "docker push max2wms/hologram-mart-client",
    "start:build": "docker-compose -f compose/docker-compose.build.yml up --build",
    "stop:build": "docker-compose -f compose/docker-compose.build.yml down",
    "start:prod": "docker-compose -f compose/docker-compose.prod.yml up",
    "stop:prod": "docker-compose -f compose/docker-compose.prod.yml down",
    "swarm:init": "docker swarm init",
    "swarm:leave": "docker swarm leave --force",
    "start:swarm": "docker stack deploy -c compose/docker-compose.stack.yml fbl",
    "stop:swarm": "docker stack rm fbl",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --code-coverage --watch",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format:fix": "pretty-quick --staged",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "postinstall": "npm rebuild node-sass"
  },
  "author": "Maxime Pierre <maxime.pierre@webmediasolutionz.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/max2-wms/hologram-mart-client.git"
  },
  "homepage": "https://github.com/max2-wms/hologram-mart-client#readme",
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~7.1.0",
    "@angular/forms": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "jquery": "^3.5.0",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.0",
    "@angular/compiler-cli": "~7.1.0",
    "@angular/language-service": "~7.1.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.10.0",
    "protractor": "~5.4.0",
    "sass-loader": "^7.1.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lerna run test",
      "pre-push": "npm test"
    }
  }
}
